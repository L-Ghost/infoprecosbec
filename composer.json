{
    "require": {
        "php": ">=7.1.3",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/dom-crawler": "^4.0",
        "symfony/console": "^4.0",
        "php-di/php-di": "^6.0",
        "laravel/lumen-framework": "5.6.*",
        "vlucas/phpdotenv": "~2.2",
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Infoprecos\\BEC\\": "src/",
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}
